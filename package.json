{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "devexp": "cross-env NODE_ENV=development PORT=3000 node server.js",
    "build": "next build",
    "start": "next start",
    "prod": "cross-env NODE_ENV=production PORT=3000 node server.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "yarn run lint -- --fix",
    "mergestag": "git checkout staging && git merge master && git push -u origin staging",
    "mergedeploy": "git checkout deploy && git merge master && git push -u origin deploy",
    "mergeall": "yarn mergestag && yarn mergedeploy"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "yarn run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
